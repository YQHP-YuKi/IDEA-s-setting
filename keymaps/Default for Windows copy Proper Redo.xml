<keymap version="1" name="Default for Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="Images.SetBackgroundImage">
    <keyboard-shortcut first-keystroke="shift ctrl alt q" />
  </action>
</keymap>